<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProgramScreen">
   <children>
      <Label layoutX="28.0" layoutY="14.0" text="Day of the Week" />
      <Label layoutX="31.0" layoutY="123.0" text="Time of Day" />
      <Label layoutX="31.0" layoutY="259.0" text="Current Setting" />
      <TextField fx:id="currentSetting" layoutX="31.0" layoutY="281.0" prefHeight="80.0" prefWidth="95.0" text="72.0">
         <font>
            <Font size="24.0" />
         </font></TextField>
      <Button fx:id="upButtonClicked" layoutX="141.0" layoutY="281.0" mnemonicParsing="false" onAction="#changeTemp" text="^" />
      <Button fx:id="downButtonClicked" layoutX="140.0" layoutY="321.0" mnemonicParsing="false" onAction="#changeTemp" text="v" />
      <Button fx:id="saveButton" layoutX="226.0" layoutY="334.0" mnemonicParsing="false" onAction="#save" text="Save">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button layoutX="300.0" layoutY="334.0" mnemonicParsing="false" text="Operate">
         <font>
            <Font size="18.0" />
         </font></Button>
      <DatePicker fx:id="dayoftheWk" layoutX="28.0" layoutY="38.0"  promptText="2019" />
      <ComboBox fx:id="timeofday" layoutX="30.0" layoutY="148.0" prefWidth="150.0" />
      <ComboBox fx:id="ampm" layoutX="186.0" layoutY="148.0" prefHeight="27.0" prefWidth="74.0" />
   </children>
</AnchorPane>
